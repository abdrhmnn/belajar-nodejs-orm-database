generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model sample {
  id Int @id
  nama String
  email String
}

model Customer {
  id String @id
  nama String
  umur Int

  @@map("customers")
}